<div class="container">
  <div class="row">
    <div class="col-xs-9">
      <h2>Books</h2>
      <table class="table">
        <tbody>
          <% @books.each do |book| %>
          <tr>
            <td><%= book.title %></td>
            <td><%= book.body %></td>
            <td><%= link_to "#",edit_book_path(book) %></td>
            <td><%= link_to "#", book_path(book) ,method: :delete %>
          </tr>
          <%end%>
        </tbody>
      </table>
    </div>
  </div>
</div>
